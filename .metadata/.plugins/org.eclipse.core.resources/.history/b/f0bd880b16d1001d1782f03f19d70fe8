package com.bikkadIT;

public class Mobile {
	
	public static void main(String[] args) {
		
		Airtel ar=new Airtel();
		ar.calling();
		ar.browse();
		
		Vodaphone vd=new Vodaphone();
		vd.calling();
		vd.browse();
		
		Sim sim=new Airtel();
		sim.calling();
		sim.browse();
		
		Sim sim1=new Vodaphone();
		sim1.calling();
		sim1.browse();
	}

}
